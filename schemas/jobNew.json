{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "title": {
            "type": "string"
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
        },
        "companyHandle": {
            "type": "string",
            "maxLength": 25
        }
    },
    "required": ["title", "companyHandle"],
    "additionalProperties": false
}